{
  "name": "ocsinventory-agent",
  "version": "0.1.0",
  "author": "puppet-admin@srce.hr",
  "summary": "ocsinventory agent pupet module",
  "license": "Apache-2.0",
  "source": "https://github.com/lutak-srce/ocsinventory",
  "project_page": "https://github.com/lutak-srce/ocsinventory",
  "issues_url": "https://github.com/lutak-srce/ocsinventory/issues",
  "dependencies": [
    {"name":"puppetlabs/stdlib","version_requirement":">= 4.13.1 < 5.0.0"}
  ],
  "data_provider": "hiera",
